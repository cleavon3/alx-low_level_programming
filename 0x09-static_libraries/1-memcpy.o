#include "main.h"

/**
 * _memcpy - Copies memory area.
 * @dest: Destination memory location.
 * @src: Source memory location.
 * @n: Number of bytes to copy.
 *
 * Return: A pointer to the destination 'dest'.
 */
char *_memcpy(char *dest, char *src, unsigned int n)
{
    unsigned int i;

    for (i = 0; i < n; i++)
    {
        dest[i] = src[i];
    }

    return (dest);
}
